{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3UploadAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Resource": "arn:aws:s3:::lazyjoshi/uploads/*"
        },
        {
            "Sid": "S3BucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::lazyjoshi"
        }
    ]
}
